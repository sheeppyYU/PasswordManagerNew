{
  "cli": {
    "version": ">= 4.0.0",
    "appVersionSource": "remote"
  },

  "build": {
    "preview": {
      "distribution": "internal",
      "ios": {},
      "android": {
        "buildType": "apk"
      }
    },

    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },

    "production": {
      "distribution": "store",
      "autoIncrement": true,
      "android": {
        "buildType": "apk"
      }
    }
  }
}
